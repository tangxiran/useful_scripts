import numpy as np
# 去除坐标轴
import matplotlib.pyplot as plt
x_label = np.array(range(10))
y_label = np.array(range(10))**2
plt.subplot(221)
plt.plot(x_label,y_label)
plt.axis('off')
plt.subplot(222)
plt.plot(x_label,y_label)
plt.axis('off')
plt.subplot(223)
plt.plot(x_label,y_label)
plt.axis('off')
plt.show()

