def draw_change(y_hist):
    import matplotlib.pyplot as plt
    x= np.arange(len(y_hist))+1
    plt.plot(list(x),y_hist)
    plt.show()
    
def draw_change(y_hist):
    import matplotlib.pyplot as plt
    x= np.arange(len(y_hist))+1
    plt.plot(list(x),y_hist)
    plt.title("min error :"+str(y_hist[-1]), fontsize=24)

    plt.show()
