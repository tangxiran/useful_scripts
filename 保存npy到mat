
def save_npt_to_mat(file = '40mode_no_duicheng_new_2020_12_30.npy',matFile='mat//mode_data'):

    import numpy as np
    file = file
    npy = np.load(file)
    import scipy.io as io
    io.savemat(matFile,{'mode_data_all':npy
                                        })

if __name__ == '__main__':

    save_npt_to_mat(file='',matfile='')
